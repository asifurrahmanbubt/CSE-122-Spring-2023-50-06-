
//Lab problem

//repetitive array

#include<iostream>
using namespace std;
int main(){
    int number;
    cin>>number;

    int array[number];

    for (int i=0;i<3;i++){
        cin>>array[i];
    }
    for (int i=3;i<number;i++){
        array[i]=array[i-3];
    }
    for(int i=0;i<number;i++){
        cout<<array[i];
    }
    return 0;
}
